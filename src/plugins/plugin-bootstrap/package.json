{
  "name": "@elizaos/plugin-bootstrap",
  "version": "1.6.2-alpha.14",
  "description": "Bootstrap plugin providing core behaviors, providers, and services for ElizaOS agents",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "typecheck": "tsc --project tsconfig.build.json --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "1.6.4",
    "unique-names-generator": "^4.7.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@elizaos/core": "1.6.4"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "@types/node": "^22.15.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": ">=1.2.0"
  },
  "publishConfig": {
    "access": "public",
    "workspaces": {
      "preserveIfPresent": true
    }
  }
}
