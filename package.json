{
  "name": "my-eliza-app",
  "version": "1.0.0",
  "description": "Custom Eliza App with React UI",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "turbo run build && bun run start-server.ts",
    "dev:watch": "turbo run dev --parallel",
    "start": "bun run start-server.ts",
    "build": "turbo run build && bun run build:backend && bun run build:frontend",
    "build:all": "turbo run build",
    "build:backend": "bun run build.ts",
    "build:frontend": "vite build",
    "type-check": "tsc --noEmit"
  },
  "packageManager": "bun@1.2.21",
  "workspaces": [
    "src/plugins/*",
    "src/packages/*"
  ],
  "dependencies": {
    "@coinbase/cdp-core": "latest",
    "@coinbase/cdp-hooks": "latest",
    "@coinbase/cdp-react": "latest",
    "@coinbase/cdp-sdk": "^1.38.4",
    "@elizaos/api-client": "workspace:*",
    "@elizaos/cli": "latest",
    "@elizaos/core": "alpha",
    "@elizaos/plugin-anthropic": "^1.0.6",
    "@elizaos/plugin-bootstrap": "workspace:*",
    "@elizaos/plugin-openai": "^1.5.15",
    "@elizaos/plugin-openrouter": "^1.5.14",
    "@elizaos/plugin-sql": "latest",
    "@elizaos/server": "workspace:*",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "latest",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@relayprotocol/relay-sdk": "^2.4.6",
    "@tanstack/react-query": "^5.29.0",
    "@tavily/core": "^0.5.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "4.1.0",
    "framer-motion": "latest",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "recharts": "latest",
    "socket.io-client": "^4.8.1",
    "streamdown": "^1.4.0",
    "tailwind-merge": "^2.6.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^13.0.0",
    "vaul": "^0.9.9",
    "viem": "^2.38.2",
    "x402-fetch": "^0.7.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.1.9",
    "turbo": "^2.3.3",
    "tw-animate-css": "1.3.3",
    "typescript": "^5.6.3",
    "vite": "^6.0.1"
  }
}
